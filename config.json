{
  // List of backend URLs for production environment
  "productionBackends": [
    "https://ton-source-prod-1.herokuapp.com",
    "https://ton-source-prod-2.herokuapp.com",
    "https://ton-source-prod-3.herokuapp.com"
  ],
  
  // List of backend URLs for testnet environment
  "testnetBackends": [
    "https://ton-source-prod-testnet-1.herokuapp.com"
  ],
  
  // Supported versions of func
  "supportedFuncVersions": [
    "0.4.6", "0.4.5", "0.4.4", "0.4.3", "0.4.2", "0.4.1", "0.4.0", "0.3.0", "0.2.0"
  ],
  
  // Supported versions of tact
  "supportedTactVersions": [
    "1.5.3", "1.5.2", "1.5.1", "1.5.0", "1.4.4", "1.4.3", "1.4.2", "1.4.1", "1.4.0", "1.3.1", "1.3.0", "1.2.0", "1.1.5", "1.1.4", "1.1.3", "1.1.2", "1.1.1", "1.1.0", "1.0.0"
  ],
  
  // Information about the Ton community
  "community": {
    "name": "Ton Community",
    "description": "A community-driven initiative for the Ton blockchain ecosystem.",
    "website": "https://ton.org"
  },
  
  // Schema validation to ensure correct types and ranges
  "schemaValidation": {
    "type": "object",
    "properties": {
      "productionBackends": {
        "type": "array",
        "items": {
          "type": "string",
          "format": "uri"
        }
      },
      "testnetBackends": {
        "type": "array",
        "items": {
          "type": "string",
          "format": "uri"
        }
      },
      "supportedFuncVersions": {
        "type": "array",
        "items": {
          "type": "string",
          "pattern": "^\\d+\\.\\d+\\.\\d+$"
        }
      },
      "supportedTactVersions": {
        "type": "array",
        "items": {
          "type": "string",
          "pattern": "^\\d+\\.\\d+\\.\\d+$"
        }
      },
      "community": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "website": {
            "type": "string",
            "format": "uri"
          }
        },
        "required": ["name", "description", "website"]
      }
    },
    "required": ["productionBackends", "testnetBackends", "supportedFuncVersions", "supportedTactVersions", "community"]
  }
}
